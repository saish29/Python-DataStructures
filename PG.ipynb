    "def fact(n):\n",
    "    assert n >= 0 and int(n) == n, \"The number must be positive integer only!\"\n",
    "    if n in [0,1]:\n",
    "        return 1\n",
    "    else:\n",
    "    \n",
    "        return n * fact(n-1)\n",
    "\n",
    "\n",
    "\n",
    "fact(25)"
   ]
